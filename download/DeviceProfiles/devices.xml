<?xml version="1.0" encoding="UTF-8" ?>

<d:devices
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:d="http://schemas.android.com/sdk/devices/1">

    <d:device>
        <d:name>SW2</d:name>
        <d:manufacturer>Sony</d:manufacturer>
        <d:hardware>
            <d:screen>
                <d:screen-size>normal</d:screen-size>
                <d:diagonal-length>1.6</d:diagonal-length>
                <d:screen-ratio>long</d:screen-ratio>
                <d:dimensions>
                    <d:x-dimension>220</d:x-dimension>
                    <d:y-dimension>176</d:y-dimension>
                </d:dimensions>
                    <d:xdpi>176</d:xdpi>
                    <d:ydpi>176</d:ydpi>
	      <d:touch>
                    <d:multitouch>none</d:multitouch>
                    <d:mechanism>notouch</d:mechanism>
                    <d:screen-type>notouch</d:screen-type>
                </d:touch>
            </d:screen>
            <d:networking>
                Bluetooth
                Nfc
            </d:networking>
            <d:sensors>
                LightSensor
                Accelerometer
                Proximity
                Compass
            <d:ram unit="MiB">256</d:ram>
            <d:cpu>ARM Cortex-M4</d:cpu>
            <d:gpu>Noinfo</d:gpu>
            <d:abi>
                armeabi
                armeabi-v7a
            </d:abi>
            <d:dock>
            </d:dock>
            <d:power-type>battery</d:power-type>
        </d:hardware>
        <d:software>
            <d:api-level>16</d:api-level>
        </d:software>
    </d:device>
</d:devices>
